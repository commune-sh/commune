{
    "name": "commune",
    "private": true,
    "scripts": {
        "build": "turbo run build",
        "dev": "turbo run dev --env-mode loose",
        "lint": "turbo run lint",
        "format": "prettier --write \"**/*.{ts,tsx,md}\"",
        "check-types": "turbo run check-types"
    },
    "devDependencies": {
        "prettier": "^3.6.2",
        "turbo": "^2.5.4",
        "typescript": "5.8.3"
    },
    "engines": {
        "node": ">=22"
    },
    "packageManager": "npm@11.6.0",
    "workspaces": [
        "apps/*",
        "packages/*"
    ]
}
